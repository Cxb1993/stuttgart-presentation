	year = {2003},
	keywords = {front tracking/finite difference, validation},
	pages = {163--182},
	annote = {The growth of a vapor bubble in superheated liquid has beenextensivelystudied in the past. The inertia-dominated growth was studied by {LordRayleigh(1917)} who solved for the momentum equation of the ambient fluid inthe absenceof viscosity and surface tension. Plesset and Zwick (1954) and Forsterand Zuber(1954) solved for the heat transfer-dominated stage using Rayleigh’sequation inconjunction with the energy equation to incorporate the variation ofbubble pres-sure with its temperature. Their solution, however, was only valid forhigh Jacobnumbers, i.e., when the thermal boundary layer around the bubble isthin comparedwith the bubble radius. This restriction was eliminated by Scriven(1959) whodeveloped a solution which is valid for entire range of Jacob {numbers.Mikic} et al.(1970) derived a general relationship for spherical bubble growth in auniformlysuperheated liquid which would tend to the limit set by Rayleigh {andPlesset} {andZwick.} Experimental studies by Dergarabedian (1960), Kosky (1968), {andFlorschuetz} et al. (1969), among others, have supported the asymptoticsolutionsdeveloped by Plesset and Zwick and Scriven.},
	file = {Esmaeeli and Tryggvason - 2003 - Computations of Explosive Boiling in Microgravity.pdf:/home/litvinov/.mozilla/firefox/hj706b5c.default/zotero/storage/IJUUD24J/Esmaeeli and Tryggvason - 2003 - Computations of Explosive Boiling in Microgravity.pdf:application/pdf;Snapshot:/home/litvinov/.mozilla/firefox/hj706b5c.default/zotero/storage/JX655TNI/10.html:text/html}
},

@article{reuther_simulating_2012,
