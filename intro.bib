@article{nikolayev_impact_2012,
	title = {Impact of the apparent contact angle on the bubble departure at boiling},
	volume = {55},
	issn = {0017-9310},
	url = {http://www.sciencedirect.com/science/article/pii/S0017931012005984},
	doi = {10.1016/j.ijheatmasstransfer.2012.07.062},
	abstract = {The contribution of the microregion (vicinity of the triple gas–liquid–solid contact line) to the force balance of a vapor bubble is discussed. The force balance of a bubble attached to the heater includes the capillary adhesion force that involves the contact angle. From the theoretical point of view, the microscopic value of the contact angle should be used in this expression. However in the experimental literature on the bubble departure, the apparent contact angle is commonly used instead. This usually leads to a substantial increase of the bubble adhesion and provides a good agreement with experimental data. A theoretical justification for such a substitution is proposed. The difference between the effective capillary adhesion force (that involving the apparent contact angle) and its true value is provided by the hydrodynamic stress caused by the liquid flow towards the contact line which is also at the origin of the difference between the microscopic and apparent contact angles.},
	number = {23–24},
	urldate = {2012-12-18},
	journal = {International Journal of Heat and Mass Transfer},
	author = {Nikolayev, {V.S.} and Janeček, V.},
	month = nov,
	year = {2012},
	keywords = {theory},
	pages = {7352--7354},
	file = {Nikolayev and Janeček - 2012 - Impact of the apparent contact angle on the bubble.pdf:/home/litvinov/.mozilla/firefox/hj706b5c.default/zotero/storage/BTCP5K7J/Nikolayev and Janeek - 2012 - Impact of the apparent contact angle on the bubble.pdf:application/pdf;ScienceDirect Snapshot:/home/litvinov/.mozilla/firefox/hj706b5c.default/zotero/storage/KAT254H6/S0017931012005984.html:text/html}
},

@article{mukherjee_numerical_2011,
